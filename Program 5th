//find common elements between two element:
class B{
	int common(int[] arr1,int[] arr2) {
		System.out.println("Common elements of arr1 and arr2 are:");
		for(int i=0;i<arr1.length;i++) {
			for(int j=0;j<arr2.length-1;j++) {
				if(arr1[i]==arr2[j])
					System.out.print(arr1[i]+" ");
			}
		}
		
		return 0;
		
	}
}
public class CommonElement {
public static void main(String[] args) {
	int[] arr1 = {6,7,5,8,4,3,9};
	int[] arr2 = {8,7,6,5,4,3,2};
	B b= new B();
	b.common(arr1, arr2);
}
}
